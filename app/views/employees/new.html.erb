<h2>New Employee</h2>
<%= form_tag "/employees", method: :post do %>
   <div>
    <%= label_tag :first_name %>
    <%= text_field_tag :first_name %>
  </div>
  <div>
    <%= label_tag :last_name %>
    <%= text_field_tag :last_name %>
  </div>
  <div>
    <%= label_tag :birthdate%>
    <%= text_field_tag :birthdate %>
  </div>
  <div>
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>
  <div>
    <%= submit_tag 'Submit!' %>
  </div>
<% end %> 
 

